public class Graph 
{
	public int counter = 0;
	
	class Node
	{
		public Node(int value) 
		{
			this.id = counter;
			this.value = value;
			this.ConnectedNodes = new Vector<Integer>();
		}
		public int value;
		public int id;
		public Vector<Integer> ConnectedNodes;
	}
	
	public static void addNode() 
	{
		
	}
	
}


